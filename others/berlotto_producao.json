mencoder your_video.flv -oac mp3lame -ovc xvid -lameopts <span class=\"nv\">preset</span><span class=\"o\">=</span>standard:fast -xvidencopts <span class=\"nv\">pass</span><span class=\"o\">=</span>1 -o your_video.avi
