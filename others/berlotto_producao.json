# Importar dump:

mysql -u username -ppassword database_name < FILE.sql ","content_highlighted": "<span class=\"c\"># Criar backup:</span>

mysqldump -u username -ppassword database_name &gt; FILE.sql 

<span class=\"c\"># Importar dump:</span>

mysql -u username -ppassword database_name &lt; FILE.sql 
","lexer": "bash","published": "2012-07-11 13:50:01","expires": "2111-02-03 13:50:01","parent_id": null,"clicks": 7,"lft": 1,"rght": 2,"tree_id": 1,"level": 0}, {"id": 2,"secret_id": "Qf9s","title": "Debug no Wordpress","author": "Sérgio","content": "#Altere/Cole as seguintes definicoes em wp-config.php
